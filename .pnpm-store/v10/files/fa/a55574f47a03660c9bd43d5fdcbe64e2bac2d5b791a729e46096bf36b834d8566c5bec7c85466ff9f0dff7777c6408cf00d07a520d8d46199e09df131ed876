const M1 = st({
  name: "BorderInput",
  components: { ColorInput: Fi, SizeInput: kl, ConfigItem: Ni },
  inject: ["designer"],
  emits: ["update:modelValue", "change"],
  props: {
    modelValue: {
      type: Object,
      default: () => ({})
    }
  },
  watch: {
    modelValue() {
      this.tidyValue();
    }
  },
  computed: {
    borderStyleStr() {
      let n = "";
      return Object.keys(this.borderStyle).forEach((l) => {
        this.borderStyle[l] !== "" && (n += Tc(l) + ": " + this.borderStyle[l] + ";");
      }, {}), n;
    },
    alignType() {
      return ["left", "center", "right"].map((n) => ({ label: this.t("props." + n), value: n }));
    },
    decorationType() {
      return ["underline", "line-through", "overline"].map((n) => ({ label: this.t("style.decoration." + n), value: n }));
    },
    weightType() {
      return [300, 400, 500, 700].map((n) => ({ label: this.t("style.weight." + n), value: n }));
    }
  },
  data() {
    return {
      t: this.designer.setupState.t,
      fontStyle: {
        fontSize: "",
        fontWeight: "",
        fontStyle: "",
        textDecoration: "",
        textAlign: "",
        lineHeight: "",
        letterSpacing: ""
      }
    };
  },
  methods: {
    tidyValue() {
      Object.keys(this.fontStyle).forEach((n) => {
        this.fontStyle[n] = this.modelValue[n] || "";
      });
    },
    onInput() {
      const n = Object.keys(this.fontStyle).reduce((l, o) => (this.fontStyle[o] !== "" && (l[o] = this.fontStyle[o]), l), {});
      this.$emit("update:modelValue", n), this.$emit("change", n);
    }
  },
  created() {
    this.tidyValue();
  }
