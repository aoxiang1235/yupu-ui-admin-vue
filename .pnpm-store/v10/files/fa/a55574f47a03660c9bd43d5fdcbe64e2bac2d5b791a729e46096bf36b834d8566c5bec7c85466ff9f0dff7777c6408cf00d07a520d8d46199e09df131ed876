function Yh(n, l) {
  const o = n.split("."), i = l.split("."), t = Math.min(o.length, i.length);
  for (var u = 0; u < t; u++) {
    var d = parseInt(o[u], 10) - parseInt(i[u], 10);
    if (d > 0)
      return 1;
    if (d < 0)
      return -1;
  }
  return o.length === i.length ? 0 : o.length < i.length ? -1 : 1;
}
