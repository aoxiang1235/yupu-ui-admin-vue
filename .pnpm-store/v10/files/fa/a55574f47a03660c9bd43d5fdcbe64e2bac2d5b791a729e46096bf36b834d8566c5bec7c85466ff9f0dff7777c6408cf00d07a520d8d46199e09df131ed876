      }), t.forEach((u) => {
        u.left > n.idx && u.left--;
      }), this.rmSlot(l, o);
    },
    splitRow(n) {
      const l = n.data.layout, o = l.row;
      if (l.row = 0, o > 1)
        for (let i = 1; i < o; i++)
          this.rule.layout.push({
            ...l,
            top: l.top + i
          });
      this.loadRule();
    },
    splitCol(n) {
      const l = n.data.layout, o = l.col;
      if (l.col = 0, o > 1)
        for (let i = 1; i < o; i++)
          this.rule.layout.push({
            ...l,
            left: l.left + i
          });
      this.loadRule();
    },
    makeMap(n) {
      let l = [];
      for (let o = n.top; o < (n.row || n.top + 1); o++)
        for (let i = n.left; i < (n.col || n.left + 1); i++)
          l.push(`${o}:${i}`);
