function Kh(n) {
  n = n || /* @__PURE__ */ new Map();
  const l = {
    $on(o, i) {
      const t = n.get(o);
      t && t.push(i) || n.set(o, [i]);
    },
    $once(o, i) {
      i._once = !0, l.$on(o, i);
    },
    $off(o, i) {
      const t = n.get(o);
      t && t.splice(t.indexOf(i) >>> 0, 1);
    },
    $emit(o, ...i) {
      (n.get(o) || []).slice().map((t) => {
        t._once && (l.$off(o, t), delete t._once), t(...i);
      }), (n.get("*") || []).slice().map((t) => {
        t(o, i);
      });
    }
  };
  return l;
}
