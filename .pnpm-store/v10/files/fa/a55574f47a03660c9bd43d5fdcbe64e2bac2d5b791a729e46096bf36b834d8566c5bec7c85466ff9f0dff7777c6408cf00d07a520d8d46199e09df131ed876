const K0 = {
  name: "FcTable",
  props: {
    label: String,
    width: [Number, String],
    border: {
      type: Boolean,
      default: !0
    },
    borderWidth: String,
    borderColor: String,
    rule: {
      type: Object,
      default: () => ({ row: 1, col: 1 })
    }
  },
  watch: {
    rule: {
      handler() {
        this.initRule(), this.loadRule(), this.tdStyle = this.rule.style || {}, this.tdClass = this.rule.class || {};
      },
      immediate: !0,
      deep: !0
    }
  },
  data() {
    return {
      tdStyle: {},
      tdClass: {},
      lattice: {}
    };
  },
  computed: {
    tableColor() {
      const n = {};
      return this.border === !1 ? n.border = "0 none" : (this.borderColor && (n.borderColor = this.borderColor), this.borderWidth && (n.borderWidth = this.borderWidth)), n;
    }
  },
  methods: {
    initRule() {
      const n = this.rule;
      n.style || (n.style = {}), n.layout || (n.layout = []), n.row || (n.row = 1), n.col || (n.col = 1);
    },
    loadRule() {
      const n = [], l = this.rule || { row: 1, col: 1 };
      for (let i = 0; i < l.row; i++) {
        const t = [];
        n.push(t);
        for (let u = 0; u < l.col; u++)
          t.push({ rowspan: 1, colspan: 1, slot: [], show: !0 });
      }
      [...l.layout || []].forEach((i, t) => {
        if ((!i.row || i.row <= 0) && (!i.col || i.col <= 0) || !n[i.top] || !n[i.top][i.left] || !n[i.top][i.left].show) {
          l.layout.splice(t, 1);
          return;
        }
        const u = n[i.top][i.left];
        u.layout = i;
        let d = 1, c = 1;
        if (i.col && (d = i.col + i.left > l.col ? l.col - i.left : i.col, u.colspan = d), i.row && (c = i.row + i.top > l.row ? l.row - i.top : i.row, u.rowspan = c), c && d)
          for (let p = 0; p < c; p++) {
            const h = n[i.top + p];
            if (h)
              for (let m = 0; m < d; m++)
                !m && !p || (h[i.left + m] && (h[i.left + m].show = !1), u.slot.push(`${i.top + p}:${i.left + m}`));
          }
      });
      const o = (i) => !!(!i || i.layout || !i.show);
      n.forEach((i, t) => {
        i.forEach((u, d) => {
          let c = !1, p = !1;
          if (u.layout) {
            const h = u.layout.col || 1, m = u.layout.row || 1;
            for (let b = 0; b < h; b++)
              if (!n[t + m] || o(n[t + m][d + b])) {
                p = !0;
                continue;
              }
            for (let b = 0; b < m; b++)
              if (!n[t + b] || o(n[t + b][d + h])) {
                c = !0;
                continue;
              }
          } else
            c = o(i[d + 1]), p = n[t + 1] ? o(n[t + 1][d]) : !0;
          u.right = c, u.bottom = p;
        });
      }), this.lattice = n;
    }
  }
