        for (let m = h.indexOf(p) - 1; m >= 0; m--) {
          if (!this.$refs[h[m]] || !this.$refs[h[m]].length)
            continue;
          const b = this.$refs[h[m]][0].list || [];
          if (b.length) {
            c = i.indexOf(b[b.length - 1]) + 1;
            break;
          }
        }
      }
      n.newIndex = c, d ? (u.slot = t, i.splice(n.newIndex, 0, u), o.added = !0, o.handleSortAfter({ rule: u })) : o.dragAdd(i, n, `${l.pid}:${l.idx}`);
    },
    dragEnd(n, l) {
      const o = this.designer.setupState, i = this.formCreateInject.children, t = n.item._underlying_vm_, u = i.indexOf(t);
      n.newIndex = u + (n.newIndex - n.oldIndex), n.oldIndex = u, o.dragEnd(this.formCreateInject.children, n, `${l.pid}:${l.idx}`);
    },
    dragStart() {
      this.designer.setupState.dragStart(this.formCreateInject.children);
    },
    dragUnchoose(n) {
      this.designer.setupState.dragUnchoose(this.formCreateInject.children, n);
    },
    initRule() {
      const n = this.rule;
      n.style || (n.style = {}), n.class || (n.class = {}), n.layout || (n.layout = []), n.row || (n.row = 1), n.col || (n.col = 1);
    },
    active(n) {
      const l = `${n.pid}:${n.idx}`;
      this.designer.setupState.customActive({
        name: "fcTableGrid",
        style: {
          formData: {
            style: this.rule.style[l] || {},
            class: this.rule.class[l] || ""
          },
          change: (o, i) => {
            this.rule[o][l] = i;
          }
        }
      });
    },
    command(n) {
      this[n[0]](...n[1]);
    },
    rmSlot(n, l) {
      const o = Object.keys(n), i = this.formCreateInject.children;
      let t = 0;
      [...i].forEach((u, d) => {
        if (!u.slot)
          return;
        let c;
        l.indexOf(u.slot) > -1 ? (i.splice(d - t, 1), t++) : (c = o.indexOf(u.slot)) > -1 && (u.slot = n[o[c]]);
      }), l.forEach((u) => {
        delete this.style[u];
      }), this.loadRule();
    },
    rmRow(n) {
      this.rule.row--;
      const l = {}, o = [];
      for (let u = n.pid; u < this.rule.row + 1; u++)
        for (let d = 0; d < this.rule.col; d++)
