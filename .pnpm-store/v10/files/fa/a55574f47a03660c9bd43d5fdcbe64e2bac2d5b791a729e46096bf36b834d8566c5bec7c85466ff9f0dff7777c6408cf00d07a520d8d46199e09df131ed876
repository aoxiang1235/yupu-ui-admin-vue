const C1 = st({
  name: "RadiusInput",
  components: { ConfigItem: Ni, ColorInput: Fi, SizeInput: kl },
  inject: ["designer"],
  emits: ["update:modelValue", "change"],
  props: {
    modelValue: String
  },
  watch: {
    modelValue(n) {
      n !== this.oldValue && this.tidyValue();
    }
  },
  computed: {
    t() {
      return this.designer.setupState.t;
    }
  },
  data() {
    return {
      visit: !1,
      active: "",
      style: {
        com: "",
        left: "",
        right: "",
        top: "",
        bottom: ""
      },
      unit: ["px", "%"],
      oldValue: ""
    };
  },
  methods: {
    batch() {
      this.style.left = this.style.com, this.style.right = this.style.com, this.style.top = this.style.com, this.style.bottom = this.style.com, this.onInput();
    },
    tidyValue() {
      if (this.style = {
        com: "",
        left: "",
        right: "",
        top: "",
        bottom: ""
      }, !this.modelValue)
        return;
      let n = (this.modelValue || "").split(" ").filter((l) => l !== "");
      n.length === 1 ? n = [n[0], n[0], n[0], n[0]] : n.length === 2 ? n = [n[0], n[1], n[0], n[1]] : n.length === 3 && (n = [n[0], n[1], n[2], n[1]]), this.style.left = n[0], this.style.top = n[1], this.style.right = n[2], this.style.bottom = n[3], this.updateCom();
    },
    updateCom() {
      let n = `${this.style.left || "0px"} ${this.style.top || "0px"} ${this.style.right || "0px"} ${this.style.bottom || "0px"}`;
      this.style.com = n.replaceAll(this.style.left, "").trim() === "" ? this.style.left : "";
    },
    onInput() {
      let n = `${this.style.left || "0px"} ${this.style.top || "0px"} ${this.style.right || "0px"} ${this.style.bottom || "0px"}`;
      `${this.style.left}${this.style.top}${this.style.right}${this.style.bottom}`.trim() === "" ? n = "" : this.updateCom(), this.oldValue = n, this.$emit("update:modelValue", n), this.$emit("change", n);
    }
  },
  created() {
    this.tidyValue();
  }
