const dA = st({
  name: "TableOptions",
  emits: ["update:modelValue", "change"],
  props: {
    modelValue: [Array, Object],
    column: {
      type: Array,
      default: () => [{ label: "label", key: "label" }, { label: "value", key: "value" }]
    },
    valueType: String,
    max: Number,
    size: String
  },
  inject: ["designer"],
  watch: {
    modelValue() {
      this.value = this.tidyModelValue();
    }
  },
  computed: {
    t() {
      return this.designer.setupState.t;
    }
  },
  data() {
    return {
      value: this.tidyModelValue()
    };
  },
  methods: {
    tidyModelValue() {
      const n = this.modelValue;
      return this.valueType === "string" ? (n || []).map((l) => ({ value: "" + l })) : this.valueType === "object" ? Object.keys(n || {}).map((l) => ({ label: l, value: n[l] })) : [...n || []].map((l) => fA(l));
    },
    tidyValue() {
      if (this.valueType === "object") {
        const n = {};
        return this.value.forEach((l) => {
          l.label && l.value && (n[l.label] = l.value);
        }), n;
      } else
        return this.value.map((n) => this.valueType === "string" ? n.value : { ...n });
    },
    onInput(n) {
      if (this.column.length === 1 && !n[this.column[0].key])
        return;
      this.column.every((o) => o.required === !1 ? !0 : ["object", "string"].indexOf(this.valueType) > -1 ? n[o.key] !== void 0 && n[o.key] !== "" && n[o.key] !== null : n[o.key] !== void 0) && this.input();
    },
    input() {
      const n = this.tidyValue();
      this.$emit("update:modelValue", n), this.$emit("change", n);
    },
    add() {
      this.value.push(this.column.reduce((n, l) => (n[l.key] = "", n), {}));
    },
    del(n) {
      this.value.splice(n, 1), this.input();
    }
  }
