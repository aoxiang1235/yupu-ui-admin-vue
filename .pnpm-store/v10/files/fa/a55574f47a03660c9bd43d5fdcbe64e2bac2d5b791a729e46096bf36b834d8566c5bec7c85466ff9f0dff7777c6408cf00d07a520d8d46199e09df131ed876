      function eh(e) {
        var r = e.split(/-(?!$)/);
        e = r[r.length - 1];
        for (var s, a, f, g, F = 0; F < r.length - 1; F++) {
          var B = r[F];
          if (/^(cmd|meta|m)$/i.test(B))
            g = !0;
